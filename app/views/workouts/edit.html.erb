<%= form_for(@work_out) do |f| %>
	<div class="field">
		<p>date</p>
		<%= f.text_field :datez%>
	</div>
	<div class="field">
		<p>meters</p>
		<%= f.text_field :meters%>
	</div>

	<div class="actions">
		<%= f.submit %>
	</div>

	<div class="field">
<%= f.hidden_field :user_id, :value =>current_user.id%>
</div>

<% end %>